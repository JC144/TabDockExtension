{
  "manifest_version": 2,
  "name": "Tab Dock",
  "version": "1.0",
  "description": "Display a dock with all tabs grouped by domain.",
  "permissions": [
    "tabs",
    "activeTab",
    "storage",
    "<all_urls>"
  ],
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {},
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.js"
      ],
      "css": [
        "dock-styles.css"
      ]
    }
  ],
  "web_accessible_resources": [
    "_favicon/*",
    "dock-styles.css",
    "images/default_favicon.png",
    "images/icons8-close.svg",
    "main.js",
    "Dock/Dock.js",
    "Dock/DockItem.js",
    "Dock/TabItem.js"
  ]
}